    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" textFill="#1ccc28" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
